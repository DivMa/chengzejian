.wrapper {
  width: 100%;
}
.wrapper .search {
   width: 96%;
   height: 40px;
   border-bottom: 1px solid #ccc;
   display: flex;
   padding: 5px 2% 5px 2%;
}
.wrapper .search input {
   flex: 10;
   height: 40px;
   border :1px solid #999;
   border-radius: 20px;
   padding-left: 10px;
   font-size: 14px;
   margin-right: 5px;
}
.wrapper .search navigator {
   font-size: 14px;
    flex:2;
    height: 40px;
    border-radius: 10px;
    line-height: 40px;
    text-align: center;
    border: 1px solid #ccc;
}
.dqdw{
  margin-top: 10px;
  margin-left:5%;
}
.rmcs{
  margin-top: 10px;
  margin-left:5%;
}
.dq{
  width: 100%;
  height: 50px;
  display: flex;
}
.dq  .info{
  width: 25%;
  margin-top: 5px;
  text-align: center;
  border-radius:20px; 
  height: 25px;
  background: #e6e6e6;
  font-size: 14px;
  line-height: 25px;
  margin-left: 5%;
}

.rm{
  width: 100%;
  height: 50px;
  display: flex;
}
.rm  .info{
  flex: 1;
  margin-top: 5px;
  text-align: center;
  border-radius:20px; 
  height: 25px;
  background: #e6e6e6;
  font-size: 14px;
  line-height: 25px;
  margin-left: 5%;
}
.rm   .info:nth-child(3){
  flex: 1;
  margin-top: 5px;
  text-align: center;
  border-radius:20px; 
  height: 25px;
  background: #e6e6e6;
  font-size: 14px;
  line-height: 25px;
  margin-left: 5%;
  margin-right: 5%;
}
